# THis is the structure to add a user name and email address
[user]
	name = <FIRST> <LAST>
	email = <EMAIL>

[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
    vdiff = difftool
    graph = log --oneline --all --graph
    wdiff = giff --color--words
[branch]
    autosetupmerge = true
[core]
    editor = vim
    excludesfile = ~/.gitignore
[url "<DASL_USER>@dasl.mem.drexel.edu:~/repos/"]
    insteadOf = dasl:
[url "git://github.com/<GITHUB_USER>/"]
    insteadOf = github:
[url "git@github.com:<GITHUB_USER>/"]
    pushInsteadOf = git://github.com/
    pushInsteadOf = github:
